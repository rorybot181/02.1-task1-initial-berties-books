<!doctype html>
<html>
  <head>
    
    <title>Welcome to <%=shopName%></title>
    <link rel="stylesheet"  type="text/css" href="/main.css" />
  </head>
  <body>
    <h1><%=shopName%> Shop</h1>
    <h2>Here are our users:</h2>
    <ul>
      <% availableUsers.forEach(function(user) { %>
        <li><%= user.username %>, <%=user.firstname%> , <%=user.lastname%>, <%=user.email%></li>
      <% }) %>
    </ul>
    </body>
</html>
